{
    "name": "vaites/wordpress-plugin-feed",
    "description": "Provides detailed feeds for WordPress plugin releases",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WordPressPluginFeed\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "5.6.0"
        }
    },
    "require": {
        "php": ">=5.6",
        "ezyang/htmlpurifier": "~4.8.0",
        "nesbot/carbon": "~1.18",
        "vlucas/phpdotenv": "~2.0",
        "symfony/dom-crawler": ">=2.6",
        "symfony/css-selector": ">=2.6",
        "symfony/console": ">=2.6",
        "symfony/serializer": ">=2.6",
        "symfony/property-access": ">=2.6",
        "symfony/process": ">=2.6",
        "symfony/yaml": ">=2.6",
        "zendframework/zend-cache": ">=2.4",
        "zendframework/zend-feed": ">=2.4",
        "zendframework/zend-http": ">=2.4",
        "zendframework/zend-servicemanager": ">=2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "symfony/var-dumper": ">=2.6",
        "squizlabs/php_codesniffer": "~2.9"
    }
}
